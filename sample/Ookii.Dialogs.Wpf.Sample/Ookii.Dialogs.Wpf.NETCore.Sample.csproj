    <ApplicationManifest>app.manifest</ApplicationManifest>
    <StartupObject>Ookii.Dialogs.Wpf.Sample.App</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\src\Ookii.Dialogs.Wpf\Ookii.Dialogs.Wpf.NETCore.csproj" />
